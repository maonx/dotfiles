snippet marginbottom "margin bottom conditions" b
<container>
	<styleBinding>
		<attribute name="width" value="-1" /> 
		<attribute name="height" value="\$marginBottom" /> 
	</styleBinding> 
	<conditions>
		<condition type="!=" left="\$marginColor" right="null">
			<styleBinding>
				<attribute name="backgroundColor" value="\$marginColor" /> 
			</styleBinding> 
		</condition> 
	</conditions> 
</container> 
endsnippet

snippet dataItems "wxmb items dataDefine" b
<dataDefine name="isAuto" type="radio" isNull="true"
						propertyName="推荐类型" description="是否是自动推荐列表">
						<config type="sync">
							<data text="自动推荐" value="true" defaultSelect="true" />
							<data text="手动推荐" value="false" defaultSelect="false" />
						</config>
					</dataDefine>
					<dataDefine name="itemIds" type="sellerGoods" isNull="false"
						propertyName="宝贝" description="宝贝" refer="isAuto:false"
						validateType="shopItemids" max="6" min="1" />
					<dataDefine name="pageSize" type="select" isNull="true"
						propertyName="宝贝个数" description="宝贝个数" refer="isAuto:true">
						<config type="sync">
							<data text="1" value="1" defaultSelect="true" />
							<data text="2" value="2" defaultSelect="false" />
							<data text="3" value="3" defaultSelect="false" />
							<data text="4" value="4" defaultSelect="false" />
							<data text="5" value="5" defaultSelect="false" />
							<data text="6" value="6" defaultSelect="false" />
						</config>

					</dataDefine>

					<dataDefine name="filterPrice" type="price" isNull="true"
						propertyName="过滤价格" description="过滤价格" refer="isAuto:true"
						validateType="price">
						<param name="startPrice" type="text" isNull="true"
							propertyName="起始价格" description="起始价格" refer="isAuto:true"
							validateType="price" />
						<param name="endPrice" type="text" isNull="true"
							propertyName="结束价格" description="结束价格" refer="isAuto:true"
							validateType="price" />
					</dataDefine>

					<dataDefine name="keyWord" type="text" max="30"
						contentSize="30" isNull="true" propertyName="关键字" description="关键字"
						refer="isAuto:true" />
					<dataDefine name="sort" type="select" isNull="true"
						propertyName="排序规则" description="排序规则" refer="isAuto:true">
						<config type="sync">
							<data text="上新" value="oldstarts" defaultSelect="true" />
							<data text="销量" value="hotsell" defaultSelect="false" />
							<data text="价格（由低到高）" value="bid" defaultSelect="false" />
						</config>
					</dataDefine>

					<dataDefine name="catId" type="select" isNull="true"
						propertyName="类目" description="类目id" refer="isAuto:true">
						<config type="async">
							<asyncData url="/decorate/ajax/select/GetShopCategorySelect.do" />
						</config>
					</dataDefine>
endsnippet

