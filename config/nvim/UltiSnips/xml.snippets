snippet itemsDate "items dataBinding" b
<!-- 数据绑定 -->
<dataBinding>
	<api needLogin="false" apiAlias="itemList" apiName="mtop.taobao.geb.enhenced.itemList.get">
		<params>
			<param name="itemIds" value="\$itemIds" />
			<param name="pageSize" value="\$pageSize" />
			<param name="auctionTag" value="\$auctionTag" />
			<param name="startPrice" value="\$filterPrice.startPrice" />
			<param name="endPrice" value="\$filterPrice.endPrice" />
			<param name="keyWord" value="\$keyWord" />
			<param name="sort" value="\$sort" />
			<param name="catId" value="\$catId" />
			<param name="pageId" value="\$pageId" />
			<param name="ownerId" value="\$userId" />
			<param name="isAuto" value="\$isAuto" />
			<!-- 1代表卖家 -->
			<param name="userType" value="1" />
		</params>
	</api>
</dataBinding>
endsnippet

